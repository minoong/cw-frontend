{
  "name": "lmw-ui",
  "version": "1.0.1",
  "description": "lmw ui",
  "keywords": [
    "lmw-ui"
  ],
  "author": "Lee minwoo <minwoowho@gmail.com>",
  "homepage": "https://github.com/minoong/lmw-ui",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minoong/lmw-ui",
    "directory": "components/lmw-ui"
  },
  "bugs": {
    "url": "https://github.com/minoong/lmw-ui/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "pnpm build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "@emotion/css": "^11.11.2",
    "clean-package": "2.1.1",
    "eslint-config-react-custom": "workspace:*",
    "tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "@emotion/css": "^11.11.2",
    "react": ">=18"
  },
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
